@model IEnumerable<ObjetoEscuelaBase>

   <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th>Id </th>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nombre</td>
                </tr>
            }
        </tbody>
    </table>